{
    "name": "@gbtunney/dawn",
    "private": true,
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "pnpm --package=@shopify/theme dlx shopify theme list",
        "\n========== COMMIT >> ==========": "",
        "commit": "pnpm run precommit && pnpm exec git-cz",
        "cz": "pnpm exec git-cz",
        "precommit": "pnpm run fix && pnpm run check && pnpm run commit:linted",
        "commit:linted": "git add $(git diff HEAD --cached --name-only)",
        "\n========== BUILD >> ==========": "",
        "build": "pnpm run vite:build",
        "vite:build": "vite build",
        "build:ts": "tsc --build",
        "\n========== DEV >> ==========": "",
        "dev": "pnpm run vite:build && pnpm run vite:serve && pnpm exec shopify theme dev",
        "vite:serve": "vite",
        "shopify:dev": "shopify theme dev --store=gbt-vite-test.myshopify.com --theme 139617829154",
        "\n========== PUBLISH >> ==========": "",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "tsc --build --clean",
        "clean:build": "rm -R -f ./assets",
        "\n========== CODE STYLE >> ==========": "",
        "lint": "cross-var eslint \"{$npm_package_config_eslint_path}.{$npm_package_config_eslint_extensions}\" --ignore-path .gitignore -c $npm_package_config_eslint_config",
        "prettier": "cross-var prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions}",
        "\n========== CHECK >> ==========": "",
        "check": "pnpm lint",
        "shopify:check": "pnpm shopify theme check",
        "\n========== FIX >> ==========": "",
        "fix": "pnpm prettier --write",
        "\n========== LIFECYCLE >> ==========": "",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gbtunney/shopify-monorepo.git"
    },
    "license": "MIT",
    "author": {
        "name": "Gillian Tunney",
        "email": "gbtunney@mac.com"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@commitlint/cz-commitlint": "^17.3.0",
        "@shopify/cli": "^3.25.0",
        "@shopify/prettier-plugin-liquid": "^1.0.0",
        "@shopify/theme": "^3.28.0",
        "@snailicide/build-config": "workspace:^0.2.4",
        "@snailicide/g-library": "workspace:^0.1.3",
        "@types/node": "^18.7.18",
        "@vitejs/plugin-vue": "^4.0.0",
        "commitizen": "^4.2.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.29.0",
        "husky": "^8.0.1",
        "prettier": "^2.8.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.1",
        "vite-plugin-shopify": "^0.0.7",
        "vite-plugin-static-copy": "^0.12.0",
        "vite-plugin-windicss": "^1.8.10",
        "vue-tsc": "^1.0.13",
        "windicss": "^3.5.6"
    },
    "config": {
        "eslint": {
            "extensions": "ts,js,cjs,mjs",
            "path": ".es*,*,**/*",
            "config": ".eslintrc.cjs"
        },
        "prettier": {
            "extensions": "liquid,ts,js,cjs,mjs,cts,json,yaml,yml,graphql,md",
            "path": "*,**/*",
            "config": "prettier.config.cjs"
        },
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "keywords": [],
    "dependencies": {
        "vue": "^3.2.45"
    }
}
